{
  "name": "svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "pages:dev": "wrangler pages dev --compatibility-date=2023-07-12 --proxy 5173 -- npm run dev",
    "pages:deploy": "NODE_VERSION=16 npm run build && wrangler pages deploy .svelte-kit/cloudflare"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-cloudflare": "^2.3.1",
    "@sveltejs/kit": "^1.5.0",
    "@types/cookie": "^0.5.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "wrangler": "^3.2.0"
  },
  "type": "module"
}